{
    "name": "kenny1911/simple-acl",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kenny1911",
            "email": "o-muzyka@mail.ru"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Kenny1911\\SimpleAcl\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kenny1911\\SimpleAcl\\Test\\": "./tests"
        }
    },
    "scripts": {
        "psalm": "./tools/psalm/vendor/bin/psalm",
        "phpunit": "phpunit",
        "php-cs-fixer": "./tools/php-cs-fixer/vendor/bin/php-cs-fixer"
    }
}
